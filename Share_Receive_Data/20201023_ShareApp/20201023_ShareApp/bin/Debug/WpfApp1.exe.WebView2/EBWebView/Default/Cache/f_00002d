<script>
function show_modal(url) {
   $('.modal .modal-body').css('overflow-y', 'auto'); 
   $('.modal .modal-dialog .modal-body').css('height', $(window).height() * 0.9);
   $("#myModal iframe").attr("src", url);
   $("#myModal").modal('show');
}
function close_modal() {
       $("#myModal iframe").attr("src", null);
}
</script>
